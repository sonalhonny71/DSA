#include<iostream>
using namespace std;
int main()
{
int i,j,n;
cin>>n;
for(i=1;i<=n;i++)
{
for(j=1;j<=i;j++)
{
cout<<j;
}
cout<<endl;
}
return 0;
}

#include<iostream>
using namespace std;
int main()
{
int i,j,n;
cin>>n;
i=1; 
while(i<=n)
{
    j=1;
    while(j<=i)
    {
    cout<<j;
    j++;
}
cout<<endl;
i++;
}
return 0;
}


